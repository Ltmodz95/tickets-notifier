<%= form_with model: ticket do |f|%>
    <div>
        <%= f.label :title%>
        <%= f.text_field :title%>
    </div>

    <div>
        <%= f.label :description%>
        <%= f.text_area :description%>
    </div>

    <div>
        <%= f.label :due_date%>
        <%= f.date_field :due_date%>
    </div>

    <div>
        <%= f.label :due_date%>
        <%= f.select :assignee, users%>
    </div>

    <%= f.submit 'Add Ticket'%>
<%end%>